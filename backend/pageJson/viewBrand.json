{
    "title": "Brands",
    "description": "List of Brands",
    "pageType": "view",
    "sendIdWithCreate": true,
    "urlFields": ["_id"],
    "fields": [{
        "name": "Name",
        "isSort": "",
        "tableRef": "name"
    }, {
        "name": "Logo Image",
        "isSort": "",
        "tableRef": "logo",
        "type": "image"
    }],
    "action": [{
        "name": "edit",
        "icon": "fa-pencil",
        "buttonClass": "btn-primary",
        "type": "page",
        "action": "editBrand",
        "fieldsToSend": {
            "_id": "_id"
        }
    }, {
        "name": "delete",
        "icon": "fa-trash",
        "buttonClass": "btn-danger",
        "type": "apiCallConfirm",
        "title": "Delete Brand",
        "content": "Are you sure you want to delete Brand?",
        "api": "Brand/delete",
        "fieldsToSend": {
            "name": "_id",
            "value": "_id"
        }
    }],
    "button": [{
        "name": "Excel Export",
        "icon": "print",
        "class": "btn-danger",
        "type": "redirect",
        "action": "Brand/generateExcel",
        "linkType": "admin"
    }, {
        "name": "Upload Excel",
        "icon": "delete",
        "class": "btn-warning",
        "type": "redirect",
        "action": "excel-upload/Brand",
        "linkType": "internal"
    }, {
        "name": "Create",
        "icon": "plus",
        "class": "btn-success",
        "type": "page",
        "action": "createBrand"
    }],
    "apiCall": {
        "url": "Brand/search",
        "params": "_id"
    }
}